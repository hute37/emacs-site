!-------------------------------------------------------------------------------
!
!	U R X V T - Settings
!
!URxvt*geometry: 92x35
URxvt*geometry:					110x35
URxvt*borderLess:				false
URxvt*depth:					32
!URxvt*scrollBar_right:				true
!URxvt*font:					xft:terminus-9
URxvt*cursorBlink:				true
URxvt*cursorUnderline:				false
!URxvt*perl-ext-common:				default,clipboard,tabbed,matcher
!URxvt*perl-ext-common:				default,tabbed,matcher
URxvt*perl-ext-common:				default,font-size
URxvt*perl-ext:					default


URxvt*print-pipe:  cat > $HOME/$(echo urxvt.dump.$(date +'%Y%M%d%H%m%S'))

URxvt.keysym.C-plus:     perl:font-size:increase
URxvt.keysym.C-minus:   perl:font-size:decrease
URxvt.keysym.C-S-plus:   perl:font-size:incglobal
URxvt.keysym.C-S-minus: perl:font-size:decglobal


!
! scroll
!
! jump scrolling
URxvt*jumpScroll: true
! skip scrolling
URxvt*skipScroll: false
! scroll to bottom on output
URxvt*scrollTtyOutput: true
! scroll to bottom when a non-special key is pressed
URxvt*scrollTtyKeypress: false
! scroll to bottom when tty receives new lines
URxvt*scrollWithBuffer: false
! scroll page by page
URxvt*mouseWheelScrollPage: false


!URxvt.scrollBar:				off
!URxvt.iconFile:					~/icons/Terminal.svg

!URxvt.tabbed.tabbar-fg:			 	7
!URxvt.tabbed.tabbar-bg: 			5
!URxvt.tabbed.tab-fg:    			4
!URxvt.tabbed.tab-bg:    			6

URxvt*termName:				 	rxvt-256color
URxvt.urlLauncher:				/usr/bin/firefox
URxvt.matcher.button:				1

URxvt*visualBell:   true
! scrollbar - true (default) or false
!URxvt*scrollBar: true
URxvt*scrollBar: true

! scrollbar position - left=false (default) or right=true
URxvt*scrollBar_right: false

! scrollbar style - rxvt (default), plain, next, or xterm
!URxvt*scrollstyle: rxvt
URxvt*scrollstyle: next


! transparency - true or false (default)
URxvt*transparent: false

! character spacing
URxvt.lineSpace: 2
URxvt.letterSpace: -1
!URxvt.letterSpace: -1



