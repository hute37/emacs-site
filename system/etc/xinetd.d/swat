service swat
{
	socket_type = stream
	protocol    = tcp
	port 	    = 901
	user        = root
#	server      = /usr/sbin/tcpd
#	server_args = /usr/sbin/swat
	server      = /usr/sbin/swat
	server_args = --debug=1 --logfile=/var/log/swat
	bind        = 127.0.0.1
	only_from   = 127.0.0.1
	wait        = no
}


