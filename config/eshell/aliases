alias fd find-dired
alias s view-file
alias less view-file
alias ll 'ls -Fhl --group-directories-first --color=always $1'
alias la 'ls -AFhl --group-directories-first --color=always $1'
alias l 'ls -1 $1'
alias gg git log --color --graph --oneline --abbrev-commit --decorate --all --date=short --pretty=format:%C(cyan)%h %C(yellow)%ad %C(bold green)%d %C(reset)%s
alias gds magit-diff-staged
alias gd magit-diff-unstaged
alias d dired
alias e find-file-other-window
alias ff find-file
